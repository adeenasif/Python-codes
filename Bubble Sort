arr = [7,3,5,10,1,6,20,17]

def bubblesort():
    for x in range(len(arr)):
        for y in range(len(arr)-1):
            if arr[y] > arr[y+1]:
                temp = arr[y]
                arr[y] =arr[y+1]
                arr[y+1] = temp

print("Before sorting: ", arr)
bubblesort()
print("After sorting: ", arr)
